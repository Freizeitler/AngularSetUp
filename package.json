{
  "name": "angularsetup",
  "version": "0.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint strombewegung",
    "tslint": "tslint \"src/**/*.ts\" \"e2e/**/*.ts\"",
    "htmllint": "htmllint \"src/**/*.html\"",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && ng run strombewegung:server:production",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "compile:server": "tsc -p server.tsconfig.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server -p 4200",
    "serve:ssr": "node dist/server",
    "postinstall": "npm run fix-ngx-charts",
    "fix-ngx-charts": "babel --presets @babel/preset-env node_modules/@swimlane/ngx-charts -d node_modules/@swimlane/ngx-charts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.4",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/platform-server": "7.1.4",
    "@angular/router": "7.1.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^7.0.0",
    "@nguniversal/common": "^7.0.2",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "core-js": "^2.6.1",
    "express": "^4.16.4",
    "picturefill": "^3.0.3",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "sass-rem": "^2.0.1",
    "smoothscroll-polyfill": "^0.4.3",
    "tslib": "^1.9.3",
    "ui-jar": "^1.1.8",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cdk": "7.2.0",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/language-service": "7.1.4",
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@types/jasmine": "^3.3.4",
    "@types/jasminewd2": "^2.0.6",
    "@types/leaflet": "^1.2.14",
    "@types/node": "^10.12.18",
    "codelyzer": "^4.5.0",
    "htmllint-cli": "0.0.7",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "node-sass": "^4.11.0",
    "protractor": "^5.4.1",
    "pump": "^3.0.0",
    "rxjs-tslint-rules": "^4.14.1",
    "stylelint": "^9.9.0",
    "stylelint-scss": "^3.4.2",
    "ts-loader": "^5.3.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0",
    "typescript": "3.1.3"
  }
